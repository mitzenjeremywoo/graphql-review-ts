      "outDir": "./dist/",
      "noImplicitAny": true,
      "target": "ESNext",
      "module": "ESNext",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
    },
    "exclude": ["node_modules"],
    "include": ["src/**/*"],
  }